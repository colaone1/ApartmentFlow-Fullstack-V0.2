    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "globals": "^16.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^13.2.1",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "supertest": "^7.1.1"
  }
}
